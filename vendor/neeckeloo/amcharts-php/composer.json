{
    "name": "neeckeloo/amcharts-php",
    "type": "library",
    "description": "PHP5 library to create AmCharts HTML5 charts.",
    "keywords": ["amcharts", "chart", "line", "pie", "bar"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Eeckeloo",
            "email": "neeckeloo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "zendframework/zend-http": "2.*",
        "zendframework/zend-json": "2.*",
        "zendframework/zend-servicemanager": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "AmCharts": ["src/", "tests/"]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
